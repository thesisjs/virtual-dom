{
  "name": "reshetaw",
  "version": "0.0.0",
  "description": "A drop-in replacement for unmaintained cito.js",
  "main": "dist/vdom.cjs.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -rf lib/ && tsc && rollup --config rollup.config.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [],
  "author": "Kaibito Young",
  "license": "MIT",
  "repository": "https://github.com/8coon/reshetaw",
  "files": [
    "dist",
    "lib"
  ],
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build && npm test"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {}
}
